{
  "testEnvironment": "node",
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "collectCoverageFrom": [
    "dist/**/*.js"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 50,
      "lines": 50,
      "functions": 50,
      "statements": 50
    }
  },
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "dist/interfaces",
    "dist/db/seeds",
    "dist/db/migrations",
    "dist/db/knexfile.js",
    "/dist/graphql/type-defs/.*"
  ]
}
