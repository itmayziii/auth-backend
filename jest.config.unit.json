{
  "testEnvironment": "node",
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "collectCoverage": true,
  "coverageThreshold": {
    "global": {
      "branches": 60,
      "lines": 60,
      "functions": 60,
      "statements": 60
    }
  },
  "collectCoverageFrom": [
    "dist/**/*.js"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "dist/interfaces",
    "dist/db/seeds",
    "dist/db/migrations",
    "dist/db/knexfile.js",
    "/dist/graphql/type-defs/.*",
    "/dist/graphql/resolvers/.*",
    "/dist/handlers/.*"
  ]
}
